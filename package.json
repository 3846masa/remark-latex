{
  "name": "@paperist/remark-latex",
  "description": "wooorm/remark plugin for latex",
  "license": "MIT",
  "version": "1.0.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm-run-all clean -p build:*",
    "build:cp": "cpx \"src/**/!(*.ts)\" lib",
